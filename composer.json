{
  "name": "remorhaz/php-unilex",
  "description": "Unilex: lexical analyzer generator with Unicode support written in PHP",
  "keywords": [
    "lex",
    "lexical analyzer",
    "lexical analyzer generator",
    "tokenizer",
    "tokenizer generator"
  ],
  "homepage": "https://github.com/remorhaz/php-unilex",
  "license": "MIT",
  "authors": [
    {
      "name": "Edward Surov",
      "email": "zoohie@gmail.com",
      "role": "Developer"
    }
  ],
  "bin": ["bin/unilex"],
  "require": {
    "php": "^7.3",
    "vanilla/garden-cli": "^2.0",
    "phpdocumentor/reflection-docblock": "^4.3.0",
    "nikic/php-parser": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2",
    "phing/phing": "^2.16",
    "squizlabs/php_codesniffer": "^3.2.0"
  },
  "autoload": {
    "psr-4": {
      "Remorhaz\\UniLex\\": ["src/", "generated/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Remorhaz\\UniLex\\Test\\": "tests/",
      "Remorhaz\\UniLex\\Example\\": ["examples/src/", "examples/generated/"],
      "Remorhaz\\UniLex\\Example\\Test\\": "examples/tests/"
    }
  }
}
