  "keywords": [
    "lex",
    "lexical analyzer",
    "lexical analyzer generator",
    "tokenizer",
    "tokenizer generator"
  ],
  "homepage": "https://github.com/remorhaz/php-json-pointer",
  "license": "MIT",
  "authors": [
    {
      "name": "Edward Surov",
      "email": "zoohie@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "~7.1"
  },
  "require-dev": {
    "phpunit/phpunit": ">=7.0.1 <8.0",
    "codeclimate/php-test-reporter": "~0.4.0"
  },
  "autoload": {
    "psr-4": {
      "Remorhaz\\UniLex\\": "src/"
    }
  },
  "autoload-dev": {
